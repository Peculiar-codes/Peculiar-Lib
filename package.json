    "autoprefixer": "^10.4.7",
    "bcryptjs": "^2.4.3",
    "bootstrap": "^5.1.3",
    "concurrently": "^7.2.1",
    "connect-flash": "^0.1.1",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^16.0.1",
    "express": "~4.16.1",
    "express-session": "^1.17.3",
    "express-validator": "^6.14.0",
    "ghooks": "^2.0.4",
    "http-errors": "~1.6.3",
