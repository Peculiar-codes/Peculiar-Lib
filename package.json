{
  "name": "app",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "tailwind:css": "tailwindcss -i ./client/src/index.css -o ./client/src/App.css",
    "server": "nodemon app.js",
    "sass": "sass --watch client/src/Stylesheets/Index.scss client/src/index.css",
    "client": "npm start --prefix client",
    "app": "concurrently \"yarn server\" \"yarn  frontend \"",
    "frontend": "concurrently \"yarn sass\" \"yarn client\""
  },
